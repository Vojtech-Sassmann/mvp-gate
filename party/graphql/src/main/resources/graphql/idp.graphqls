enum IdpOrderField {
    NAME
}

input IdpOrder {
    direction: OrderDirection!
    field: IdpOrderField
}

input CreateIdpInput {
    name: String!
    loginUrl: String!
}

type Idp {
    name: String!
    id: ID!
    loginUrl: String!
}

type IdpConnection {
    totalCount: Int!
    nodes: [Idp!]!
}
